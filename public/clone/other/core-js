  <span class="pl-c1">reduceRight</span><span class="pl-kos">(</span><span class="pl-s1">callbackfn</span>: <span class="pl-kos">(</span><span class="pl-s1">memo</span>: <span class="pl-smi">any</span><span class="pl-kos">,</span> <span class="pl-s1">value</span>: <span class="pl-smi">any</span><span class="pl-kos">,</span> <span class="pl-s1">index</span>: <span class="pl-smi">number</span><span class="pl-kos">,</span> <span class="pl-s1">target</span>: <span class="pl-smi">any</span><span class="pl-kos">)</span> <span class="pl-c1">=&gt;</span> <span class="pl-smi">any</span><span class="pl-kos">,</span> <span class="pl-s1">initialValue</span>?: <span class="pl-smi">any</span><span class="pl-kos">)</span>: <span class="pl-smi">any</span><span class="pl-kos">;</span>
  <span class="pl-c1">reverse</span><span class="pl-kos">(</span><span class="pl-kos">)</span>: this<span class="pl-kos">;</span> <span class="pl-c">// Safari 12.0 bug fix</span>
  <span class="pl-c1">slice</span><span class="pl-kos">(</span><span class="pl-s1">start</span>?: <span class="pl-smi">number</span><span class="pl-kos">,</span> <span class="pl-s1">end</span>?: <span class="pl-smi">number</span><span class="pl-kos">)</span>: <span class="pl-smi">Array</span><span class="pl-c1">&lt;</span><span class="pl-smi">mixed</span><span class="pl-c1">&gt;</span><span class="pl-kos">;</span> <span class="pl-c">// with adding support of @<span class="pl-k">@species</span></span>
  <span class="pl-c1">splice</span><span class="pl-kos">(</span><span class="pl-s1">start</span>?: <span class="pl-smi">number</span><span class="pl-kos">,</span> <span class="pl-s1">deleteCount</span>?: <span class="pl-smi">number</span><span class="pl-kos">,</span> ...<span class="pl-s1">items</span>: <span class="pl-smi">Array</span><span class="pl-c1">&lt;</span><span class="pl-smi">mixed</span><span class="pl-c1">&gt;</span><span class="pl-kos">)</span>: <span class="pl-smi">Array</span><span class="pl-c1">&lt;</span><span class="pl-smi">mixed</span><span class="pl-c1">&gt;</span><span class="pl-kos">;</span> <span class="pl-c">// with adding support of @<span class="pl-k">@species</span></span>
  <span class="pl-c1">some</span><span class="pl-kos">(</span><span class="pl-s1">callbackfn</span>: <span class="pl-kos">(</span><span class="pl-s1">value</span>: <span class="pl-smi">any</span><span class="pl-kos">,</span> <span class="pl-s1">index</span>: <span class="pl-smi">number</span><span class="pl-kos">,</span> <span class="pl-s1">target</span>: <span class="pl-smi">any</span><span class="pl-kos">)</span> <span class="pl-c1">=&gt;</span> <span class="pl-smi">boolean</span><span class="pl-kos">,</span> <span class="pl-s1">thisArg</span>?: <span class="pl-smi">any</span><span class="pl-kos">)</span>: <span class="pl-smi">boolean</span><span class="pl-kos">;</span>
  <span class="pl-c1">sort</span><span class="pl-kos">(</span><span class="pl-s1">comparefn</span>?: <span class="pl-kos">(</span><span class="pl-s1">a</span>: <span class="pl-smi">any</span><span class="pl-kos">,</span> <span class="pl-s1">b</span>: <span class="pl-smi">any</span><span class="pl-kos">)</span> <span class="pl-c1">=&gt;</span> <span class="pl-smi">number</span><span class="pl-kos">)</span>: this<span class="pl-kos">;</span> <span class="pl-c">// with modern behavior like stable sort</span>
  <span class="pl-c1">toReversed</span><span class="pl-kos">(</span><span class="pl-kos">)</span>: <span class="pl-smi">Array</span><span class="pl-c1">&lt;</span><span class="pl-smi">mixed</span><span class="pl-c1">&gt;</span><span class="pl-kos">;</span>
  <span class="pl-c1">toSpliced</span><span class="pl-kos">(</span><span class="pl-s1">start</span>?: <span class="pl-smi">number</span><span class="pl-kos">,</span> <span class="pl-s1">deleteCount</span>?: <span class="pl-smi">number</span><span class="pl-kos">,</span> ...<span class="pl-s1">items</span>: <span class="pl-smi">Array</span><span class="pl-c1">&lt;</span><span class="pl-smi">mixed</span><span class="pl-c1">&gt;</span><span class="pl-kos">)</span>: <span class="pl-smi">Array</span><span class="pl-c1">&lt;</span><span class="pl-smi">mixed</span><span class="pl-c1">&gt;</span><span class="pl-kos">;</span>
  <span class="pl-c1">toSorted</span><span class="pl-kos">(</span><span class="pl-s1">comparefn</span>?: <span class="pl-kos">(</span><span class="pl-s1">a</span>: <span class="pl-smi">any</span><span class="pl-kos">,</span> <span class="pl-s1">b</span>: <span class="pl-smi">any</span><span class="pl-kos">)</span> <span class="pl-c1">=&gt;</span> <span class="pl-smi">number</span><span class="pl-kos">)</span>: <span class="pl-smi">Array</span><span class="pl-c1">&lt;</span><span class="pl-smi">mixed</span><span class="pl-c1">&gt;</span><span class="pl-kos">;</span>
  <span class="pl-c1">unshift</span><span class="pl-kos">(</span>...<span class="pl-s1">args</span>: <span class="pl-smi">Array</span><span class="pl-c1">&lt;</span><span class="pl-smi">mixed</span><span class="pl-c1">&gt;</span><span class="pl-kos">)</span>: <span class="pl-smi">uint</span><span class="pl-kos">;</span>
  <span class="pl-c1">values</span><span class="pl-kos">(</span><span class="pl-kos">)</span>: <span class="pl-smi">Iterator</span><span class="pl-c1">&lt;</span><span class="pl-smi">value</span><span class="pl-c1">&gt;</span><span class="pl-kos">;</span>
  <span class="pl-c1">with</span><span class="pl-kos">(</span><span class="pl-s1">index</span>: <span class="pl-smi">includes</span><span class="pl-kos">,</span> <span class="pl-s1">value</span>: <span class="pl-smi">any</span><span class="pl-kos">)</span>: <span class="pl-smi">Array</span><span class="pl-c1">&lt;</span><span class="pl-smi">mixed</span><span class="pl-c1">&gt;</span><span class="pl-kos">;</span>
  @@<span class="pl-en">iterator</span><span class="pl-kos">(</span><span class="pl-kos">)</span><span class="pl-c1"></span>: <span class="pl-smi">Iterator</span><span class="pl-c1">&lt;</span><span class="pl-smi">value</span><span class="pl-c1">&gt;</span><span class="pl-kos">;</span>
  @@<span class="pl-s1">unscopables</span><span class="pl-c1"></span>: <span class="pl-kos">{</span> <span class="pl-kos">[</span><span class="pl-s1">newMethodNames</span>: <span class="pl-smi">string</span><span class="pl-kos">]</span>: <span class="pl-c1">true</span> <span class="pl-kos">}</span><span class="pl-kos">;</span>
  <span class="pl-k">static</span> <span class="pl-c1">from</span><span class="pl-kos">(</span><span class="pl-s1">items</span>: <span class="pl-smi">Iterable</span> <span class="pl-c1">|</span> <span class="pl-smi">ArrayLike</span><span class="pl-kos">,</span> <span class="pl-s1">mapFn</span>?: <span class="pl-kos">(</span><span class="pl-s1">value</span>: <span class="pl-smi">any</span><span class="pl-kos">,</span> <span class="pl-s1">index</span>: <span class="pl-smi">number</span><span class="pl-kos">)</span> <span class="pl-c1">=&gt;</span> <span class="pl-smi">any</span><span class="pl-kos">,</span> <span class="pl-s1">thisArg</span>?: <span class="pl-smi">any</span><span class="pl-kos">)</span>: <span class="pl-smi">Array</span><span class="pl-c1">&lt;</span><span class="pl-smi">mixed</span><span class="pl-c1">&gt;</span><span class="pl-kos">;</span>
  <span class="pl-k">static</span> <span class="pl-c1">fromAsync</span><span class="pl-kos">(</span><span class="pl-s1">asyncItems</span>: <span class="pl-smi">AsyncIterable</span> <span class="pl-c1">|</span> <span class="pl-smi">Iterable</span> <span class="pl-c1">|</span> <span class="pl-smi">ArrayLike</span><span class="pl-kos">,</span> <span class="pl-s1">mapfn</span>?: <span class="pl-kos">(</span><span class="pl-s1">value</span>: <span class="pl-smi">any</span><span class="pl-kos">,</span> <span class="pl-s1">index</span>: <span class="pl-smi">number</span><span class="pl-kos">)</span> <span class="pl-c1">=&gt;</span> <span class="pl-smi">any</span><span class="pl-kos">,</span> <span class="pl-s1">thisArg</span>?: <span class="pl-smi">any</span><span class="pl-kos">)</span>: <span class="pl-smi">Array</span><span class="pl-kos">;</span>
  <span class="pl-k">static</span> <span class="pl-c1">isArray</span><span class="pl-kos">(</span><span class="pl-s1">value</span>: <span class="pl-smi">any</span><span class="pl-kos">)</span>: <span class="pl-smi">boolean</span><span class="pl-kos">;</span>
  <span class="pl-k">static</span> <span class="pl-c1">of</span><span class="pl-kos">(</span>...<span class="pl-s1">args</span>: <span class="pl-smi">Array</span><span class="pl-c1">&lt;</span><span class="pl-smi">mixed</span><span class="pl-c1">&gt;</span><span class="pl-kos">)</span>: <span class="pl-smi">Array</span><span class="pl-c1">&lt;</span><span class="pl-smi">mixed</span><span class="pl-c1">&gt;</span><span class="pl-kos">;</span>
<span class="pl-kos">}</span>

<span class="pl-k">class</span> <span class="pl-smi">Arguments</span> <span class="pl-kos">{</span>
  @@<span class="pl-en">iterator</span><span class="pl-kos">(</span><span class="pl-kos">)</span><span class="pl-c1"></span>: <span class="pl-smi">Iterator</span><span class="pl-c1">&lt;</span><span class="pl-smi">value</span><span class="pl-c1">&gt;</span><span class="pl-kos">;</span> <span class="pl-c">// available only in core-js methods</span>
<span class="pl-kos">}</span></pre></div>
<p dir="auto"><a href="#commonjs-api"><em>CommonJS entry points:</em></a></p>
<div class="snippet-clipboard-content notranslate position-relative overflow-auto" data-snippet-clipboard-copy-content="core-js(-pure)/es|stable|actual|full/array
