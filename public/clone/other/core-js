atob('aGksIGNvcmUtanM='); // =&gt; 'hi, core-js'"><pre><span class="pl-en">btoa</span><span class="pl-kos">(</span><span class="pl-s">'hi, core-js'</span><span class="pl-kos">)</span><span class="pl-kos">;</span>      <span class="pl-c">// =&gt; 'aGksIGNvcmUtanM='</span>
