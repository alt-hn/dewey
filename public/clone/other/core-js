  <span class="pl-c1">attribute</span> <span class="pl-s1">href</span>: <span class="pl-smi">string</span><span class="pl-kos">;</span>
  <span class="pl-k">readonly</span> <span class="pl-c1">attribute</span> <span class="pl-s1">origin</span>: <span class="pl-smi">string</span><span class="pl-kos">;</span>
  <span class="pl-c1">attribute</span> <span class="pl-s1">protocol</span>: <span class="pl-smi">string</span><span class="pl-kos">;</span>
  <span class="pl-c1">attribute</span> <span class="pl-s1">username</span>: <span class="pl-smi">string</span><span class="pl-kos">;</span>
  <span class="pl-c1">attribute</span> <span class="pl-s1">password</span>: <span class="pl-smi">string</span><span class="pl-kos">;</span>
  <span class="pl-c1">attribute</span> <span class="pl-s1">host</span>: <span class="pl-smi">string</span><span class="pl-kos">;</span>
  <span class="pl-c1">attribute</span> <span class="pl-s1">hostname</span>: <span class="pl-smi">string</span><span class="pl-kos">;</span>
  <span class="pl-c1">attribute</span> <span class="pl-s1">port</span>: <span class="pl-smi">string</span><span class="pl-kos">;</span>
