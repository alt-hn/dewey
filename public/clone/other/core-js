  <span class="pl-c1">toString</span><span class="pl-kos">(</span><span class="pl-kos">)</span>: <span class="pl-smi">string</span><span class="pl-kos">;</span> <span class="pl-c">// different fixes</span>
<span class="pl-kos">}</span>

<span class="pl-k">class</span> <span class="pl-kos">[</span>
