  static isError(value: any): boolean;
}"><pre><span class="pl-k">class</span> <span class="pl-smi">Error</span> <span class="pl-kos">{</span>
  <span class="pl-k">static</span> <span class="pl-c1">isError</span><span class="pl-kos">(</span><span class="pl-s1">value</span>: <span class="pl-smi">any</span><span class="pl-kos">)</span>: <span class="pl-smi">boolean</span><span class="pl-kos">;</span>
