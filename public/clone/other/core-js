  Promise.resolve(1),
  Promise.reject(2),
  Promise.resolve(3),
]).then(console.log); // =&gt; [{ value: 1, status: 'fulfilled' }, { reason: 2, status: 'rejected' }, { value: 3, status: 'fulfilled' }]"><pre><span class="pl-v">Promise</span><span class="pl-kos">.</span><span class="pl-en">allSettled</span><span class="pl-kos">(</span><span class="pl-kos">[</span>
