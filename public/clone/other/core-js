  <span class="pl-c1">push</span><span class="pl-kos">(</span>...<span class="pl-s1">args</span>: <span class="pl-smi">Array</span><span class="pl-c1">&lt;</span><span class="pl-smi">mixed</span><span class="pl-c1">&gt;</span><span class="pl-kos">)</span>: <span class="pl-smi">uint</span><span class="pl-kos">;</span>
