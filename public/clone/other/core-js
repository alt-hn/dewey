  zip&lt;T extends readonly Iterable&lt;unknown&gt;[]&gt;(
    iterables: T,
    options?: {
      mode?: 'shortest' | 'longest' | 'strict';
      padding?: { [K in keyof T]?: T[K] extends Iterable&lt;infer U&gt; ? U : never };
