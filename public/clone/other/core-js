  @@<span class="pl-en">iterator</span><span class="pl-kos">(</span><span class="pl-kos">)</span>: <span class="pl-v">Iterator</span><span class="pl-c1">&lt;</span><span class="pl-smi">value</span><span class="pl-c1">&gt;</span><span class="pl-kos">;</span>
