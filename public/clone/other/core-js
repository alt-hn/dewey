<span class="pl-kos">}</span>

<span class="pl-k">class</span> <span class="pl-kos">[</span><span class="pl-v">DOMTokenList</span><span class="pl-kos">,</span> <span class="pl-v">NodeList</span><span class="pl-kos">]</span> <span class="pl-kos">{</span>
