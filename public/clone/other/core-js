  <span class="pl-c1">entries</span><span class="pl-kos">(</span><span class="pl-kos">)</span>: <span class="pl-smi">Iterator</span><span class="pl-c1">&lt;</span><span class="pl-kos">[</span><span class="pl-smi">value</span><span class="pl-kos">,</span> <span class="pl-smi">value</span><span class="pl-kos">]</span><span class="pl-c1">&gt;</span><span class="pl-kos">;</span>
