<span class="pl-v">Function</span><span class="pl-kos">.</span><span class="pl-en">isCallable</span><span class="pl-kos">(</span><span class="pl-kos">(</span><span class="pl-kos">)</span> <span class="pl-c1">=&gt;</span> <span class="pl-kos">{</span> <span class="pl-c">/* empty */</span> <span class="pl-kos">}</span><span class="pl-kos">)</span><span class="pl-kos">;</span>       <span class="pl-c">// =&gt; true</span>
