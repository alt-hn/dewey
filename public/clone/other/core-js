<p dir="auto">Basic <a href="https://tinyurl.com/23bhbhbu" rel="nofollow"><em>example</em></a>:</p>
<div class="highlight highlight-source-js notranslate position-relative overflow-auto" dir="auto" data-snippet-clipboard-copy-content="/* eslint-disable promise/prefer-await-to-callbacks -- example */
function sleepRandom(time) {
  return new Promise((resolve, reject) =&gt; {
    setTimeout(resolve, time * 1e3, 0 | Math.random() * 1e3);
  });
}

console.log('Run');                    // =&gt; Run
sleepRandom(5).then(result =&gt; {
  console.log(result);                 // =&gt; 869, after 5 sec.
  return sleepRandom(10);
}).then(result =&gt; {
  console.log(result);                 // =&gt; 202, after 10 sec.
}).then(() =&gt; {
  console.log('immediately after');    // =&gt; immediately after
  throw new Error('Irror!');
}).then(() =&gt; {
  console.log('will not be displayed');
}).catch(error =&gt; console.log(error)); // =&gt; =&gt; Error: Irror!"><pre><span class="pl-c">/* eslint-disable promise/prefer-await-to-callbacks -- example */</span>
